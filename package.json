{
  "name": "restapi-typescript-decorators",
  "version": "6.1.0",
  "description": "Decorators that helps reduce the boilderplate code for rest api calls on the frontend and node js",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rm -rf build/* && tsc",
    "format": "prettier --config ./.prettierrc --write src/**/**/**/**/*.{ts,tsx,js,jsx,scss} mock-server/**/**/*.js *.json *.js .prettierrc *.md ",
    "clean": "jest --clearCache && rm -rf build/*",
    "test": "jest --ci",
    "test:resetSnapshot": "jest --ci --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synle/restapi-typescript-decorators.git"
  },
  "keywords": [
    "restapi",
    "fetch",
    "decorator",
    "annotation"
  ],
  "author": "Sy Le",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/synle/restapi-typescript-decorators/issues"
  },
  "homepage": "https://github.com/synle/restapi-typescript-decorators#readme",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "fast-xml-parser": "^3.17.4",
    "form-data": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.7",
    "@types/qs": "^6.9.3",
    "jest": "^26.6.3",
    "prettier": "1.19.1",
    "ts-jest": "^26.5.0",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {}
}
